function onDrop(x, y, dragdata)
	if dragdata.isType("shortcut") then
		local sClass, sRecord = dragdata.getShortcutData();
		setValue(sClass, sRecord);

		if StringManager.contains({"background"}, sClass) then
			local node = DB.findNode(sRecord);
			if node then
				window.background.setValue(DB.getChild(node, "name").getValue());
			end
		end
		return true;		
	end
end
