    <LUACode>
local HealthNode = nil;
local StabilityNode = nil;

function linkHealthStability()
	-- Initialize Health and stability field links
	local abilitiesNode = getDatabaseNode().getChild('.abilities.generalsections');
	
	HealthNode = GumshoeManager.getGeneralAbilityNode(abilitiesNode, GumshoeData.getHealthAbility());
	healthName.setValue(HealthNode.getChild("name").getValue());
	healthRating.setLink(HealthNode.getChild("rating"), true);
	healthPool.setLink(HealthNode.getChild("pool"), true);

	StabilityNode = GumshoeManager.getGeneralAbilityNode(abilitiesNode, GumshoeData.getStabilityAbility());
	stabilityName.setValue(StabilityNode.getChild("name").getValue());
	stabilityRating.setLink(StabilityNode.getChild("rating"), true);
	stabilityPool.setLink(StabilityNode.getChild("pool"), true);
end

function getHealthNode()
	return HealthNode;
end

function getStabilityNode()
	return StabilityNode;
end

</LUACode>
