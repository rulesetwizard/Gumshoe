	listAbilities_iedit.setVisible(bEdit);
	sectionName.setReadOnly(not bEdit);
	if not bEdit then
		listAbilities_iedit.setValue(0);
		spacerHeader.setVisible(bEdit);
	end
	for _,w in ipairs(listAbilities.getWindows()) do
		w.setEdit(bEdit);
	end
end</LUACode>
