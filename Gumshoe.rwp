	super.onInit();
	local values = '';
	for _, ability in pairs(GumshoeData.getCombatAbilities()) do
		if values == '' then
			values = ability;
		else
			values = values .. '|' .. ability;
		end
	end
	initialize(values, values, '', '');
	synchData();	
	updateDisplay();	
end
